{
  "name": "path-ai-by-mal-backend",
  "version": "5.3.0",
  "main": "dist/app.js",
  "type": "module",
  "imports": {
    "#db/*": {
      "development": "./src/db/*.ts",
      "default": "./dist/db/*.js"
    },
    "#db": {
      "development": "./src/db/index.ts",
      "default": "./dist/db/index.js"
    },
    "#models/*": {
      "development": "./src/models/*.ts",
      "default": "./dist/models/*.js"
    },
    "#models": {
      "development": "./src/models/index.ts",
      "default": "./dist/models/index.js"
    },
    "#controllers/*": {
      "development": "./src/controllers/*.ts",
      "default": "./dist/controllers/*.js"
    },
    "#controllers": {
      "development": "./src/controllers/index.ts",
      "default": "./dist/controllers/index.js"
    },
    "#routers/*": {
      "development": "./src/routers/*.ts",
      "default": "./dist/routers/*.js"
    },
    "#routers": {
      "development": "./src/routers/index.ts",
      "default": "./dist/routers/index.js"
    },
    "#schemas/*": {
      "development": "./src/schemas/*.ts",
      "default": "./dist/schemas/*.js"
    },
    "#schemas": {
      "development": "./src/schemas/index.ts",
      "default": "./dist/schemas/index.js"
    },
    "#middleware/*": {
      "development": "./src/middleware/*.ts",
      "default": "./dist/middleware/*.js"
    },
    "#middleware": {
      "development": "./src/middleware/index.ts",
      "default": "./dist/middleware/index.js"
    },
    "#utils/*": {
      "development": "./src/utils/*.ts",
      "default": "./dist/utils/*.js"
    },
    "#utils": {
      "development": "./src/utils/index.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "dev": "node --env-file=.env.development.local --watch --conditions development src/app.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node --env-file=.env.production.local dist/app.js",
    "test": "jest --passWithNoTests",
    "migrate:changelog": "ts-node scripts/migrate-changelog.ts",
    "add:changelog": "ts-node scripts/add-changelog.ts"
  },
  "description": "PathAI backend â€” APIs for Learning Dashboard, Templates, and Focus Mode",
  "keywords": [
    "learning",
    "dashboard",
    "focus-mode",
    "templates",
    "backend",
    "edtech"
  ],
  "author": {
    "name": "Malick",
    "email": "me@malick.cloud",
    "url": "https://pathai.one"
  },
  "license": "PolyForm-Noncommercial-1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaaintmal/path-ai.git"
  },
  "bugs": {
    "url": "https://github.com/yaaintmal/path-ai/issues"
  },
  "homepage": "https://github.com/yaaintmal/path-ai#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.5",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "typescript": "^5.8.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.4",
    "supertest": "^7.1.4",
    "@types/supertest": "^6.0.3"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.27.0",
    "bcrypt": "^6.0.0",
    "chalk": "^5.6.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.1",
    "multer": "^2.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.1.12"
  }
}
